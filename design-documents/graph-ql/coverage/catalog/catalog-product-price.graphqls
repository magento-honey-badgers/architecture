type ProductPriceRange {
    minimum_price: ProductPrice!
    maximum_price: ProductPrice
}

type ProductPrice {
    regular_price: Money!
    final_price: Money!
    discount: ProductDiscount
    fixed_product_taxes: [FixedProductTax] #added in Weee module
    msrp: Money #added in msrp module
}

type FixedProductTax {
    amount: Money
    label: String
}

type ProductDiscount {
    percent_off:Float
    amount_off: Float
}